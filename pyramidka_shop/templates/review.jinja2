{% block content %}
<div class="orders" style="
                          margin: 0 auto; 
                          width: 80%; 
                          text-align: center;
                          padding-top: 50px;
                          ">
    <h1>Заказы</h1>
    <table border="1" style="margin: 0 auto;">
      <tr>
        <td>Номер</td>
        <td>Имя</td>
        <td>Телефон</td>
        <td>Cостав</td>
        <td>Цена</td>
      </tr>
      {% for record in info %}
      <tr>
        <td>{{ record.id }}</td>
        <td>{{ record.name }}</td>
        <td>{{ record.phone }}</td>
        <td>{{ record.tea_list }}</td>
        <td>{{ record.price }}</td>
      </tr>
        {% endfor %}
    </table>
</div>
{% endblock content %}